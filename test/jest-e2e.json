{
  "moduleFileExtensions": ["js", "json", "ts"],
  "roots": [".", "../test"],
  "rootDir": "../src",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "testPathIgnorePatterns": ["/test/e2e"],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverage": true,
  "collectCoverageFrom": ["**/*.ts", "!main.ts"]
}
