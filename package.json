{
  "name": "etuutt-api-nestjs-prisma",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "npx nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npx nest start",
    "start:dev": "npx nest start --watch",
    "start:debug": "npx nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config test/jest.json -t",
    "test:unit": "jest --config test/jest.json \"unit/app.spec.ts\" -t",
    "test:e2e": "jest --config test/jest.json \"e2e/app.e2e-spec.ts\" -t",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "db:reset": "env-cmd -f .env.dev --use-shell \"npx prisma migrate reset --force && prisma db push\"",
    "test:db:reset": "env-cmd -f .env.test --use-shell \"npx prisma migrate reset --force && prisma db push\"",
    "db:editor": "env-cmd -f .env.dev --use-shell \"npx prisma studio\"",
    "test:db:editor": "env-cmd -f .env.test --use-shell \"npx prisma studio\""
  },
  "dependencies": {
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^9.4.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/swagger": "^6.3.0",
    "@prisma/client": "^5.7.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "pactum-matchers": "^1.1.6",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/passport-jwt": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cz-emoji": "1.3.2-canary.2",
    "env-cmd": "^10.1.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "pactum": "^3.5.1",
    "prettier": "^2.8.8",
    "prisma": "^5.7.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  }
}
